<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<div class="refresh">
			<g:Button ui:field="refresh">Refresh</g:Button>
		</div>


		<fieldset class="infoblock">
			<legend>Prints annotated</legend>
			<h3>Total prints annotated</h3>
			<g:Label ui:field="lblTotalAnnotated"></g:Label>
			<h3>Latest (10) prints</h3>
			<g:VerticalPanel ui:field="pnlAnnotated"></g:VerticalPanel>
		</fieldset>
		<fieldset class="infoblock">
			<legend>Expertise</legend>
			<h3>Flora expertise</h3>
			<div id="floraSlider" class="expertiseSlider"></div>
			<h3>Castle expertise</h3>
			<div id="castleSlider" class="expertiseSlider"></div>
		</fieldset>
	</g:HTMLPanel>

</ui:UiBinder>