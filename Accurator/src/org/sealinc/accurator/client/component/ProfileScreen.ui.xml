<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default" xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<fieldset class="infoblock">
			<legend>
				<ui:msg description="Prints annotated">Prints annotated</ui:msg>
			</legend>
			<h3>
				<ui:msg description="Total prints annotated">Total prints annotated</ui:msg>
			</h3>
			<g:Label ui:field="lblTotalAnnotated"></g:Label>
			<h3>
				<ui:msg description="Latest (10) prints">Latest (10) prints</ui:msg>
			</h3>
			<g:VerticalPanel ui:field="pnlAnnotated"></g:VerticalPanel>
		</fieldset>
		<fieldset class="infoblock">
			<legend>
				<ui:msg description="Expertise">Expertise</ui:msg>
			</legend>
			<h3>
				<ui:msg description="Flora expertise">Flora expertise</ui:msg>
			</h3>
			<div id="floraSlider" class="expertiseSlider"></div>
			<h3>
				<ui:msg description="Castle expertise">Castle expertise</ui:msg>
			</h3>
			<div id="castleSlider" class="expertiseSlider"></div>
		</fieldset>
		<fieldset class="infoblock">
			<legend>
				<ui:msg description="Preferences">Preferences</ui:msg>
			</legend>
			<h3><ui:msg description="Language">Language</ui:msg></h3>
			<div id="language">
				<input type="radio" id="languageEN" name="language" class="languageButton" checked="checked"
					value="en" />
				<label for="languageEN"><ui:msg description="English">English</ui:msg></label>
				<input type="radio" id="languageNL" value="nl" name="language" class="languageButton"  />
				<label for="languageNL"><ui:msg description="Dutch">Dutch</ui:msg></label>
				<p><em><ui:msg description="Message to indicate pagereload">Will reload the page</ui:msg></em></p>
			</div>
		</fieldset>
	</g:HTMLPanel>

</ui:UiBinder>